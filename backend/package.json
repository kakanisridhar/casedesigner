{
  "name": "@case-designer/backend",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=DEV nest start",
    "dev": "cross-env NODE_ENV=DEV nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=dev jest",
    "test:watch": "cross-env NODE_ENV=TEST jest --watch",
    "test:cov": "cross-env NODE_ENV=TEST jest --coverage",
    "test:debug": "cross-env NODE_ENV=TEST E2E=true node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand src",
    "test:e2e": "cross-env NODE_ENV=TEST E2E=true jest --runInBand"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^3.0.6",
    "@nestjs/common": "^7.1.1",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.1.1",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.1.1",
    "@nestjs/terminus": "^7.1.0",
    "@nestjs/typeorm": "^7.1.0",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "helmet": "^3.22.0",
    "nest-winston": "^1.3.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.2.1",
    "ramda": "^0.27.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "ts-auto-mock": "^2.1.22",
    "typeorm": "^0.2.25",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^4.1.12",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/passport-jwt": "^3.0.3",
    "@types/pg": "^7.14.3",
    "@types/ramda": "^0.27.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "faker": "^4.1.0",
    "jest": "^26.6.3",
    "jest-chain": "^1.1.5",
    "jest-extended": "^0.11.5",
    "jest-ts-auto-mock": "^1.0.11",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "type-fest": "^0.15.0"
  }
}
